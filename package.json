{
  "name": "sql-ts-knex-generator",
  "version": "1.4.0-beta.0",
  "main": "./dist/index.js",
  "license": "MIT",
  "description": "Generate TypeScript definitions (https://github.com/rmp135/sql-ts) and Knex.js(https://github.com/knex/knex) query helper functions from database schemas.",
  "bin": "./bin/sql-ts",
  "types": "dist/index.d.ts",
  "repository": {
    "url": "https://github.com/saostad/sql-ts-knex-generator",
    "type": "git"
  },
  "author": {
    "name": "Saeid Ostad"
  },
  "scripts": {
    "build": "tsc",
    "prebuild": "copyfiles -f ./src/templates/*.handlebars ./dist/templates/",
    "pretest": "tsc -p tsconfig.test.json",
    "test": "jasmine ./__test/{**,}/*.spec.js"
  },
  "dependencies": {
    "handlebars": "^4.7.2",
    "knex": "^0.20.8",
    "mssql": "^6.0.1",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/handlebars": "^4.0.39",
    "@types/jasmine": "^3.5.1",
    "@types/knex": "^0.16.1",
    "@types/rewire": "^2.5.28",
    "@types/yargs": "^15.0.0",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "copyfiles": "^2.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fast-node-logger": "^1.0.1",
    "jasmine": "^3.1.0",
    "prettier": "^1.19.1",
    "rewire": "^4.0.1",
    "typescript": "^3.7.5"
  }
}
